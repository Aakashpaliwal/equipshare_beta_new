  <script>
function find_subcategory(val){
  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && (this.status == 200 || this.status == 304)) {
  
  var subcat=new Array();
  subcat=JSON.parse(this.responseText);

  
  var select = document.getElementById('subcat');
       for(var i = select.options.length - 1 ; i > 0 ; i--) {
         select.remove(i);
        }
  
  for(var i=0;i<subcat.length;i++) { 
            option = document.createElement('option');
            option.value =subcat[i].subcategory ;
            option.text = subcat[i].subcategory ;
            select.appendChild(option);
     }
    }
  };
  xhttp.open("GET", "/user_add_equipment_category?category="+val,true);
  xhttp.send();
}


function find_brand(val){

  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && (this.status == 200 || this.status == 304)) {
  
  var brand=new Array();
  brand=JSON.parse(this.responseText);

  var select = document.getElementById('brand');
       for(var i = select.options.length - 1 ; i > 0 ; i--) {
         select.remove(i);
}
  
  for(var i=0;i<brand.length;i++) { 
    
        option = document.createElement('option');
            option.value =brand[i].brand ;
            option.text = brand[i].brand ;
            select.appendChild(option);
     }
    }
  };
  var category = document.getElementById('cat').value;
  xhttp.open("GET", "/user_add_equipment_subcategory?subcategory="+val+'&category='+category,true);
  xhttp.send();
}


function find_model(val){
  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && (this.status == 200 || this.status == 304)) {
  var model=new Array();
  model=JSON.parse(this.responseText);

  
  var select = document.getElementById('model');
       for(var i = select.options.length - 1 ; i > 0 ; i--) {
         select.remove(i);
}
  
  for(var i=0;i<model.length;i++) { 
        option = document.createElement('option');
            option.value =model[i].model ;
            option.text = model[i].model ;
            select.appendChild(option);
     }
    }
  };
  var category = document.getElementById('cat').value;
  var subcategory = document.getElementById('subcat').value;
  xhttp.open("GET", "/user_add_equipment_brand?brand="+val+'&category='+category+'&subcategory='+subcategory,true);
  xhttp.send();
}

</script>          
